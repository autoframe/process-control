{
    "type": "library",
    "name": "autoframe/process-control",
    "description": "Process child worker, execution lock check, Autoframe Framework",
    "license": "MIT",
    "homepage": "https://github.com/autoframe",
    "require": {
        "php": ">=7.4"
    },
    "version": "1.0.1",
    "authors": [
        {
            "name": "Nistor Alexandru Marius",
            "homepage": "https://autoframe.ro",
            "role": "Developer"
        }
    ],
    "keywords": [
        "child worker",
        "child thread",
        "child spawn",
        "execution lock",
        "inplace lock",
        "pid lock"
    ],
    "autoload": {
        "psr-4": {
            "Autoframe\\Process\\Control\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Unit\\": "Tests/Unit"
        }
    },
    "require-dev": {
        "phpunit/phpunit": ">=9.6"
    },
    "optimize-autoloader": false
}
